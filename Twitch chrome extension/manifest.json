{
  "manifest_version": 2,

  "name": "Twitch Api",
  "description": "Extension to find the top league of legend streamers",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "tabs",
    "https://ajax.googleapis.com/",
    "https://*/*","http://*/*"
  ]
}
